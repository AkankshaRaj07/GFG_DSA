class Solution {
    public int maxSubarrayXOR(int[] arr, int k) {
        // code here
        if(k>arr.length ) return -1;
        int xor =0;
        for(int i = 0; i<k ; i++){
            xor ^=arr[i];
        }
        int maxXor=xor;
        for(int i =k;i<arr.length;  i++){
            xor=xor^arr[i-k] ^arr[i];
            maxXor=Math.max(xor,maxXor);
        }
        
        return maxXor;
        
    }
}
